<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sal.oss_mobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sal.oss_mobile.StimulusActivity"></activity>
        <activity android:name="com.sal.oss_mobile.BarChartActivity"></activity>
        <activity android:name="com.sal.oss_mobile.WayActivity"></activity>
        <activity android:name="com.sal.oss_mobile.MainActivity"></activity>
        <activity android:name="com.sal.oss_mobile.SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.sal.oss_mobile.MainActivity2"></activity>
    </application>

</manifest>